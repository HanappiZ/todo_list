/ul>\n    <p class=\"none\" v-else>Add a new todo in the input above</p>\n  </div>\n</template>\n\n<script>\nexport default {\n  name: 'App',\n  components: {\n  },\n  data () {\n      return {\n          newTodoText: \"\",\n          todos: []\n      }\n  },\n  methods: {\n      addTodo () {\n        if (this.newTodoText) {\n          this.todos.push({\n            text: this.newTodoText,\n            id: Date.now()\n          })\n          this.newTodoText = ''\n        }\n      },\n      removeTodo (item) {\n        this.todos = this.todos.filter(_item => _item !== item)\n      }\n  }\n}\n</script>\n\n\n<style>\n</style>\n"],"sourceRoot":""}]}